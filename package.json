{
  "name": "babylon_parcel_template",
  "version": "1.0.0",
  "description": "",
  "source": "index.html",
  "type": "module",
  "scripts": {
    "dev": "parcel ",
    "build": "parcel build --no-source-maps --no-cache --public-url public/",
    "build:hoist": "parcel build --no-source-maps --no-scope-hoist --no-cache --public-url public/"
  },
  "staticFiles": {
    "staticPath": "public",
    "staticOutPath": "public",
    "watcherGlob": "**"
  },
  "@parcel/bundler-default": {
    "minBundles": 1,
    "minBundleSize": 5000,
    "maxParallelRequests": 20
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.10.3",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@babylonjs/core": "^6.40.0",
    "@babylonjs/havok": "^1.3.0",
    "@babylonjs/inspector": "^6.40.0",
    "@babylonjs/loaders": "^6.40.0",
    "@babylonjs/procedural-textures": "^6.42.0",
    "pixi.js": "^7.4.0"
  }
}
